<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SmartExecutor xmlns:ns2="http://www.armangal.com/smartexecutor/v1">

	<executor>
		<name>Default Executor</name>
		<description>Default Executor used in the system</description>
		<pools>
			<pool>
				<poolName>Default</poolName>
				<poolNameShort>DTP</poolNameShort>
				<corePollSize>2</corePollSize>
				<maxPoolSize>3</maxPoolSize>
				<queueSize>100</queueSize>
				<keepAliveTime>1000</keepAliveTime>
				<poolType>regular</poolType>
				<chunks>10</chunks>
				<chunkInterval>10000</chunkInterval>
			</pool>

			<pool>
				<poolName>Custom1</poolName>
				<poolNameShort>CM1</poolNameShort>
				<corePollSize>2</corePollSize>
				<maxPoolSize>2</maxPoolSize>
				<queueSize>10</queueSize>
				<keepAliveTime>1000</keepAliveTime>
				<poolType>regular</poolType>
				<chunks>200</chunks>
				<chunkInterval>100</chunkInterval>
			</pool>
			
			<pool>
				<poolName>Fast</poolName>
				<poolNameShort>FST</poolNameShort>
				<corePollSize>2</corePollSize>
				<maxPoolSize>2</maxPoolSize>
				<queueSize>100</queueSize>
				<keepAliveTime>1000</keepAliveTime>
				<poolType>regular</poolType>
				<chunks>2000</chunks>
				<chunkInterval>100</chunkInterval>
			</pool>
			
			<pool>
				<poolName>Scheduled</poolName>
				<poolNameShort>STP1</poolNameShort>
				<corePollSize>5</corePollSize>
				<poolType>scheduled</poolType>
				<chunks>10</chunks>
				<chunkInterval>10000</chunkInterval>
			</pool>

			<pool>
				<poolName>Scheduled2</poolName>
				<poolNameShort>STP2</poolNameShort>
				<corePollSize>5</corePollSize>
				<poolType>scheduled</poolType>
				<chunks>10</chunks>
				<chunkInterval>10000</chunkInterval>
			</pool>
			<pool>
				<poolName>Cached1</poolName>
				<poolNameShort>CTP</poolNameShort>
				<poolType>cached</poolType>
				<chunks>10</chunks>
				<chunkInterval>10000</chunkInterval>
			</pool>
		</pools>
	</executor>
</SmartExecutor>